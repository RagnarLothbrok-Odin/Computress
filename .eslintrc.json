{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "no-use-before-define": 0,
        "no-console": 0,
        "no-plusplus": 0,
        "max-len": 0
    }
}
